{
  "name": "hnpwa-gatsby",
  "description": "Hacker News - Gatsby",
  "version": "1.0.0",
  "author": "Emmanuel Fallancy <emmanuel.fallancy@gmail.com>",
  "dependencies": {
    "axios": "^0.18.0",
    "gatsby": "^1.9.241",
    "gatsby-link": "^1.6.39",
    "gatsby-plugin-glamor": "^1.6.13",
    "gatsby-plugin-manifest": "^1.0.19",
    "gatsby-plugin-netlify": "^1.0.19",
    "gatsby-plugin-offline": "^1.0.15",
    "gatsby-plugin-react-helmet": "^2.0.8",
    "gatsby-plugin-remove-trailing-slashes": "^1.0.9",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby",
    "hnpwa"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format:all": "npm run format:src && npm run format:gatsby",
    "format:src": "prettier-eslint --write 'src/**/*.js'",
